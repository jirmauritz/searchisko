{
  "aggregations" : {
    "top_contributors" : {
      "global" : { },
      "aggregations" : {
        "top_contributors_filter" : {
          "filter" : {
            "and" : {
              "filters" : [ {
                "terms" : {
                  "sys_tags" : [ "tag2", "tag1" ]
                }
              } ]
            }
          },
          "aggregations" : {
            "top_contributors_buckets" : {
              "terms" : {
                "field" : "sys_contributors",
                "size" : 100
              }
            }
          }
        }
      }
    }
  }
}